<div class="edit-drawer">
    <p class="edit-drawer-cancel-btn"><i class="fa-solid fa-xmark"></i></p>
    <div class="edit-drawer-form">
        <h2 class="title">Edit your Wish List</h2>
        <form class="wish-list-drawer-form">
            <div class="edit-drawer-wish-list">
                <p>Wish List Name</p>
                <input type="text" />
            </div>
            <div class="wish-list-status">
                <p>Wish List Status</p>
                <div class="list-status">
                    <label>
                        <input type="radio"
                               name="status"
                               value="Private" />Private(Only you can see)
                    </label>
                    <label>
                        <input type="radio"
                               name="status"
                               value="Public" />Public(Everybody can see)
                    </label>
                </div>
            </div>
            <div class="edit-drawer-save-btn">
                <button type="submit" class="save"><p>Save</p></button>
                <button class="cancel-edit-drawer">Cancel</button>
            </div>
        </form>
    </div>
</div>
